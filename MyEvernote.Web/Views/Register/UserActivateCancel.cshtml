@using MyEvernote.Entities.Messages
@model List<ErrorMessageObj>
@{
    ViewBag.Title = "Geçersiz İşlem";
}

<link href="~/Content/css/Login-Register.css" rel="stylesheet" />
<div class="container" style="margin-top:80px;">
    <div class="row">
        <div class="col-sm-6 col-md-4 col-md-offset-4 outlined">
            <h1 class="text-center login-title">Yönlendiriliyorsunuz...</h1>
            <div class="account-wall">
                <div class="text-danger">
                    <h2><span class="glyphicon glyphicon-remove"></span>&nbsp;&nbsp;&nbsp;Geçersiz İşlem</h2>
                    <hr />
                    <p class="text-justify">
                       @foreach (ErrorMessageObj err in Model)
                       { 
                           @err.Message<br>
                       }
                    </p>
                </div>
            </div>
        </div>
    </div>
</div>
@section scripts{
    <script>
        $(function () {
            $("footer").hide();

            setTimeout(function () {
                window.location = "/Register/Register";
            }, 5000)
        });
    </script>
}

